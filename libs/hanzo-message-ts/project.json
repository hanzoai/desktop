{
  "name": "hanzo-message-ts",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/hanzo-message-ts/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/hanzo-message-ts",
        "main": "libs/hanzo-message-ts/src/index.ts",
        "tsConfig": "libs/hanzo-message-ts/tsconfig.lib.json",
        "minimal": true,
        "generateLockfile": true
      }
    },
    "serve": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/hanzo-message-ts",
        "main": "libs/hanzo-message-ts/src/index.ts",
        "tsConfig": "libs/hanzo-message-ts/tsconfig.lib.json",
        "minimal": true,
        "generateLockfile": true
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/hanzo-message-ts/**/*.ts",
          "libs/hanzo-message-ts/package.json"
        ]
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "config": "libs/hanzo-message-ts/vite.config.ts"
      }
    }
  }
}
